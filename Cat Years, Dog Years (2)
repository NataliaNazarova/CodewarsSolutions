// Cat Years, Dog Years (2)
// https://www.codewars.com/kata/5a6d3bd238f80014a2000187/train/javascript
let ownedCatAndDog = function(catYears, dogYears) {
  let cat = catYears < 15 ? 0: catYears < 24 ? 1: Math.floor(2 + (catYears - 24) / 4);
  let dog = dogYears < 15 ? 0: dogYears < 24 ? 1: Math.floor(2 + (dogYears - 24) / 5);
  return [cat,dog];
}
