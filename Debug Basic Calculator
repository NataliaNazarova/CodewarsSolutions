// Debug Basic Calculator
// https://www.codewars.com/kata/56368f37d464c0a43c00007f/train/javascript

let calculate = function calculate(a, o, b) {
 let result = 0;
 
 if(o === "+") { 
   return a + b;
   } else if(o === "-") {
       return a - b;
     } else if(o === "/" && b != 0) { 
         return a / b;
       } else if(o === "*") {
           return a * b;
         }
 return null; 
}
