// How old will I be in 2099?
// https://www.codewars.com/kata/5761a717780f8950ce001473/train/javascript

function  calculateAge(year1, year2){
  return year1 === year2 ? 'You were born this very year!' : year1 > year2 ? `You will be born in ${year1 - year2} year${year1 - year2 > 1 ? 's' : ''}.` : `You are ${year2 - year1} year${year2 - year1 > 1 ? 's' : ''} old.` 
}
