// Is there a vowel in there?
// https://www.codewars.com/kata/57cff961eca260b71900008f/train/javascript

function isVow(a){
  let arr = [];
  for(let i = 0; i < a.length; i++){
    if(a[i] === 97 || a[i] === 101 || a[i] === 105 || a[i] === 111 || a[i] === 117) arr.push(String.fromCharCode(a[i]));
    else arr.push(a[i]);
  }
  return arr;
}
